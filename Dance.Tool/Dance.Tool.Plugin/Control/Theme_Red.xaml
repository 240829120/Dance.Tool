<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:dance="http://schemas.dance.com/wpf">

    <!-- 背景 -->
    <SolidColorBrush x:Key="{x:Static Member=dance:DanceResourceKey.BACKGROUND_BRUSH}">#FFFFFFFF</SolidColorBrush>
    <SolidColorBrush x:Key="{x:Static Member=dance:DanceResourceKey.BACKGROUND_BRUSH_SELECTED}">#FFDB0413</SolidColorBrush>
    <SolidColorBrush x:Key="{x:Static Member=dance:DanceResourceKey.BACKGROUND_BRUSH_MOUSE_OVER}">#44DB0413</SolidColorBrush>
    <SolidColorBrush x:Key="{x:Static Member=dance:DanceResourceKey.BACKGROUND_BRUSH_PRESSED}">#22DB0413</SolidColorBrush>

    <!-- 边框 -->
    <SolidColorBrush x:Key="{x:Static Member=dance:DanceResourceKey.BORDER_BRUSH}">#33767676</SolidColorBrush>
    <SolidColorBrush x:Key="{x:Static Member=dance:DanceResourceKey.BORDER_BRUSH_SELECTED}">#FFDB0413</SolidColorBrush>
    <SolidColorBrush x:Key="{x:Static Member=dance:DanceResourceKey.BORDER_BRUSH_MOUSE_OVER}">#44DB0413</SolidColorBrush>
    <SolidColorBrush x:Key="{x:Static Member=dance:DanceResourceKey.BORDER_BRUSH_PRESSED}">#22DB0413</SolidColorBrush>

</ResourceDictionary>